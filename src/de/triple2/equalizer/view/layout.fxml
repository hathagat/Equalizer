<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.media.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.triple2.equalizer.view.LayoutController">
   <center>
      <HBox alignment="CENTER" prefHeight="250.0" prefWidth="580.0">
         <children>
            <BorderPane>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <top>
                  <Label fx:id="labeldBTopLeft" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="5 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </top>
               <bottom>
                  <Label fx:id="labeldBBottomLeft" alignment="BOTTOM_RIGHT" contentDisplay="RIGHT" text="-5 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </bottom>
               <center>
                  <Label alignment="BOTTOM_RIGHT" contentDisplay="RIGHT" text="0 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </center>
            </BorderPane>
            <Slider fx:id="slider1" blockIncrement="0.5" layoutX="10.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider2" blockIncrement="0.5" layoutX="126.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider3" blockIncrement="0.5" layoutX="20.0" layoutY="20.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider4" blockIncrement="0.5" layoutX="20.0" layoutY="20.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider5" blockIncrement="0.5" layoutX="54.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider6" blockIncrement="0.5" layoutX="78.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider7" blockIncrement="0.5" layoutX="126.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider8" blockIncrement="0.5" layoutX="102.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider9" blockIncrement="0.5" layoutX="150.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <Slider fx:id="slider10" blockIncrement="0.5" layoutX="174.0" layoutY="10.0" majorTickUnit="5.0" max="5.0" min="-5.0" minorTickCount="10" orientation="VERTICAL" showTickMarks="true" snapToTicks="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
            </Slider>
            <BorderPane layoutX="72.0" layoutY="20.0">
               <top>
                  <Label fx:id="labeldBTopRight" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="5 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER_RIGHT">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </top>
               <bottom>
                  <Label fx:id="labeldBBottomRight" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="-5 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER_RIGHT">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </bottom>
               <center>
                  <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="0 dB" textAlignment="RIGHT" textFill="#757575" BorderPane.alignment="CENTER_RIGHT">
                     <font>
                        <Font size="10.0" />
                     </font>
                  </Label>
               </center>
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </BorderPane>
         </children>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" prefHeight="70.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelClipping" textFill="RED">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labelName">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="labelTime" layoutX="480.0" layoutY="52.0">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="buttonPlay" disable="true" mnemonicParsing="false" onAction="#handleButtonPlay" text="Abspielen">
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Datei">
            <items>
              <MenuItem fx:id="menuItemOpen" mnemonicParsing="false" onAction="#handleMenuOpen" text="Öffnen" />
                  <MenuItem fx:id="menuItemClose" mnemonicParsing="false" onAction="#handleMenuClose" text="Beenden" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Einstellungen">
            <items>
                  <Menu mnemonicParsing="false" text="Verstärkung">
                     <items>
                        <RadioMenuItem fx:id="radioMenuItemGain5" mnemonicParsing="false" onAction="#handleMenuGain" selected="true" text="+/- 5 dB">
                           <toggleGroup>
                              <ToggleGroup fx:id="toggleGroupGain" />
                           </toggleGroup>
                        </RadioMenuItem>
                        <RadioMenuItem fx:id="radioMenuItemGain10" mnemonicParsing="false" onAction="#handleMenuGain" text="+/- 10 dB" toggleGroup="$toggleGroupGain" />
                        <RadioMenuItem fx:id="radioMenuItemGain15" mnemonicParsing="false" onAction="#handleMenuGain" text="+/- 15 dB" toggleGroup="$toggleGroupGain" />
                     </items>
                  </Menu>
                  <Menu fx:id="menuSlider" mnemonicParsing="false" text="Bänderanzahl">
                     <items>
                        <RadioMenuItem fx:id="radioMenuItemSlider5" mnemonicParsing="false" onAction="#handleMenuSlider" text="5 Bänder">
                           <toggleGroup>
                              <ToggleGroup fx:id="toggleGroupSlider" />
                           </toggleGroup>
                        </RadioMenuItem>
                        <RadioMenuItem fx:id="radioMenuItemSlider7" mnemonicParsing="false" onAction="#handleMenuSlider" text="7 Bänder" toggleGroup="$toggleGroupSlider" />
                        <RadioMenuItem fx:id="radioMenuItemSlider10" mnemonicParsing="false" onAction="#handleMenuSlider" selected="true" text="10 Bänder" toggleGroup="$toggleGroupSlider" />
                     </items>
                  </Menu>
                  <Menu fx:id="menuBuffer" mnemonicParsing="false" text="Buffergröße">
                     <items>
                        <RadioMenuItem fx:id="radioMenuItemBuffer1024" mnemonicParsing="false" onAction="#handleMenuBuffer" text="1024 Frames">
                           <toggleGroup>
                              <ToggleGroup fx:id="toggleGroupBuffer" />
                           </toggleGroup></RadioMenuItem>
                        <RadioMenuItem fx:id="radioMenuItemBuffer4096" mnemonicParsing="false" onAction="#handleMenuBuffer" text="4096 Frames" toggleGroup="$toggleGroupBuffer" />
                        <RadioMenuItem fx:id="radioMenuItemBuffer8192" mnemonicParsing="false" onAction="#handleMenuBuffer" selected="true" text="8192 Frames" toggleGroup="$toggleGroupBuffer" />
                        <RadioMenuItem fx:id="radioMenuItemBuffer16384" mnemonicParsing="false" onAction="#handleMenuBuffer" text="16384 Frames" toggleGroup="$toggleGroupBuffer" />
                     </items>
                  </Menu>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Hilfe">
            <items>
              <MenuItem fx:id="menuItemAbout" mnemonicParsing="false" text="Über" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
